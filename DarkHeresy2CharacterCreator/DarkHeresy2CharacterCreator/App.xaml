<Application x:Class="DarkHeresy2CharacterCreator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DarkHeresy2CharacterCreator"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Dictionaries/ColorsDictionary.xaml"/>
                <ResourceDictionary Source="View/Dictionaries/ElementsDictionary.xaml"/>
                <ResourceDictionary Source="View/Dictionaries/FontsDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
