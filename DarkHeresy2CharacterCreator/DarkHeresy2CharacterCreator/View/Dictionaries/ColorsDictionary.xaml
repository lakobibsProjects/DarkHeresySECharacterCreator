<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:t="clr-namespace:DarkHeresy2CharacterCreator.View"
                    xmlns:local="clr-namespace:DarkHeresy2CharacterCreator.View.Dictionaries"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <Color x:Key="Bronze">#CD7F32</Color>
    <Color x:Key="Bronze_Dark">#784511</Color>
    <Color x:Key="Blood_Fresh">#FF980002</Color>    <!--compiler add ff to begin of codding-->
    <Color x:Key="Blood_Dried">#4b0101</Color>
    <Color x:Key="Black">#000000</Color>
    <Color x:Key="White">#FFFFFF</Color>
    <Color x:Key="Blood_Red">#FF980000</Color>    <!--compiler add ff to begin of codding-->
    <SolidColorBrush x:Key="DarkBrush">#FFFFFF</SolidColorBrush>
    
    <LinearGradientBrush x:Key="BronzeButtonBackground" StartPoint="1,0.3" EndPoint="0,0">
        <GradientStop Color="{DynamicResource ResourceKey=Bronze}" Offset=".7" />
        <GradientStop Color="{DynamicResource ResourceKey=Bronze_Dark}" Offset="0" />
    </LinearGradientBrush>
</ResourceDictionary>